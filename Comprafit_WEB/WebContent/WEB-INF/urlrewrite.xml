
    <?xml version="1.0" encoding="utf-8"?>

    <!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

    <urlrewrite>

       <rule>
		 <from>e\.key-([0-9]+).jsp</from>
		 <to >/index.xhtml</to>
		</rule>
		<rule >
			<from>/([a-z]+)([a-z0-9_]+)([a-z]+)$</from>
			<to >/index.xhtml</to>
		</rule>
		
    </urlrewrite>
